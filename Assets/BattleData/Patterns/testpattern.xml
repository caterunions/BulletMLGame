<?xml version="1.0" ?>
<bulletml>
  <action label="top1">
    <actionRef label="testac1"/>
  </action>

  <bullet label="test">
    <direction type="absolute">0</direction>
    <speed>2</speed>
    <faceHeading/>
    <action>
      <wait>1500</wait>
      <changeSpeed>
        <speed>0</speed>
      </changeSpeed>
      <rotate>
        <rotRate>90</rotRate>
      </rotate>
      <wait>1000</wait>
      <rotate>
        <rotRate>0</rotRate>
      </rotate>
    </action>
  </bullet>

  <action label="testac1">
    <repeat>
      <times>25</times>
      <action>
        <fire>
          <bulletRef label="test"/>
        </fire>
      <wait>100</wait>
      </action>
    </repeat>
  </action>

  <action label="testac2">
    <repeat>
      <times>50</times>
      <action>
        <fire>
          <speed>10</speed>
          <bulletRef label="test"/>
        </fire>
        <wait>500</wait>
        </action>
      </repeat>
  </action>
</bulletml>